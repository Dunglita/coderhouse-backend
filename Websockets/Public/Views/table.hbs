    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">Titulo</th>
          <th scope="col">Precio</th>
          <th scope="col">Imagen</th>
        </tr>
      </thead>
      <tbody>
        {{#each item}}
          <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img
                src={{this.thumbnail}}
                alt={{this.title}}
                width="30"
                height="30"
              /></td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>